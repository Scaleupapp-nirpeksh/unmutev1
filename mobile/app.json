{
    "expo": {
      "name": "Unmute",
      "slug": "unmute",
      "version": "0.0.1",
      "orientation": "portrait",
      "jsEngine": "hermes",
      "userInterfaceStyle": "automatic",
      "sdkVersion": "50.0.0",
      "platforms": ["ios", "android"],
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": ["**/*"],
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/adaptive-icon.png",
          "backgroundColor": "#FFFFFF"
        }
      }
    }
  }